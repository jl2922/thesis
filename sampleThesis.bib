file = {:Users/sandeepsharma/Library/Application Support/Mendeley Desktop/Downloaded/Guo et al. - 2016 - N-Electron Valence State Perturbation Theory Based on a Density Matrix Renormalization Group Reference Function, wit.pdf:pdf},
issn = {1549-9626},
journal = {J. Chem. Theory Comput.},
language = {EN},
number = {4},
